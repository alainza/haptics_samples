{
  "bindings": {
    "/actions/default": {
      "haptics": [
        {
          "output": "/actions/default/out/haptic",
          "path": "/user/hand/right/output/haptic"
        }
      ],
      "sources": [
        {
          "inputs": {
            "click": {
              "output": "/actions/default/in/press1"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/a"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/default/in/press2"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/b"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/default/in/press1"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/x"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/default/in/press2"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/y"
        },
        {
          "inputs": {
            "pull": {
              "output": "/actions/default/in/analog1"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/right/input/trigger"
        },
        {
          "inputs": {
            "pull": {
              "output": "/actions/default/in/analog1"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/left/input/trigger"
        }
      ]
    }
  },
  "controller_type": "hpmotioncontroller",
  "description": "HelloVR Bindings for HP controller",
  "name": "HelloVR bindings for HP Controller"
}
